#include <iostream>
using namespace std;
int main(){
    cout<<'\t'<<"Palindrome check"<<endl<<endl;
    int x;
    cout<<"Enter the number: ";
    cin>>x;
    if(x>=0&&x<=9){
        cout<<"Palindrome";
        return 0;
    }
    int y=x, count=0;
    while(y!=0){
        y=y/10;
        count++;
    }
    
    y=x;
    
    for(int i=1; i<=count; i++){
        if((y/(10^(count-1)) ==x%(10^i))){
            x=x/10;
            y=y%(10^(count-1));
        }
        else{
            cout<<"Not Palindrome!"<<endl;
            return 0;
        }
    }
    return 0;
}
